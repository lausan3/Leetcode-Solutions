class Solution:
    def asteroidCollision(self, asteroids: List[int]) -> List[int]:
        result = []

        for asteroid in asteroids:
            if not result or asteroid > 0 or (result and result[-1] < 0):
                result.append(asteroid)
            else:
                while result and asteroid * -1 >= result[-1]:
                    result.pop()
            
        return result